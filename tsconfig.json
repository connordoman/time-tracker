{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "baseUrl": ".",
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "paths": {
            "@lib/*": ["src/lib/*"],
            "@components/*": ["src/components/*"],
            "@styles/*": ["src/styles/*"],
            "@pages/*": ["src/pages/*"]
        }
    }
}
